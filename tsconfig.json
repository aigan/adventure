{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "target": "ES2024",
    "module": "ES2022",
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "strictFunctionTypes": false
  },
  "include": ["public/worker/**/*.mjs"],
  "exclude": ["node_modules"]
}
